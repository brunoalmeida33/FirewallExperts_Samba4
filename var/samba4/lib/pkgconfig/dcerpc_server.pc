prefix=/var/samba4
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/include
modulesdir=${prefix}/lib/dcerpc_server

Name: dcerpc_server
Description: DCE/RPC server library
Requires: dcerpc
Version: 0.0.1
Libs: -Wl,-rpath,/var/samba4/lib -L${libdir} -ldcerpc-server
Cflags: -I${includedir} -DHAVE_IMMEDIATE_STRUCTURES=1
